<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.sistema_votacion_escolar.Controllers.LoginController">
    <children>
        <Label text="Login" />
        <TextField fx:id="identificationNumberField" promptText="Número de Identificación" />
        <Button text="Iniciar Sesión" onAction="#handleLoginButtonAction" />
        <Label fx:id="messageLabel"/>
    </children>
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
</VBox>